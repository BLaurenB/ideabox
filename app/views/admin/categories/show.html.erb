<h1><%= @category.title%></h1>

<h2><%= @category.title%>'s Ideas</h2>
<% @category.ideas.each do |idea| %>
<h3><%= idea.title %></h3>
<%= button_to "Edit", edit_admin_category_path(@category), method: :get %>
<%= button_to "Delete", admin_category_path(@category), method: :delete %>
<% end %>
<br/>

<%= link_to "Return to Main Category Page", admin_categories_path %>
